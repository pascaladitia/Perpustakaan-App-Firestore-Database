<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.examplel.perpustakaanapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PerpustakaanApp"
        android:usesCleartextTraffic="true">
        <activity android:name=".ui.home.DetailBukuActivity"></activity>
        <activity android:name=".ui.home.DetailActivity" />
        <activity android:name=".ui.home.user.UserActivity" />
        <activity android:name=".ui.login.RegisterActivity" />
        <activity android:name=".ui.login.LoginActivity" />
        <activity android:name=".ui.home.InputActivity" />
        <activity android:name=".ui.home.HomeActivity" />
        <activity android:name=".ui.home.InputBukuActivity" />
        <activity android:name=".ui.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>